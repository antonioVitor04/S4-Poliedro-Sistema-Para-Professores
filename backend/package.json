{
  "name": "backend",
  "version": "1.0.0",
  "main": "server.cjs",
  "scripts": {
    "start": "node server.cjs",
    "dev": "nodemon server.cjs",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test:unit": "cross-env NODE_ENV=test jest --testPathPattern=tests/unit --runInBand",
    "test:integration": "cross-env NODE_ENV=test jest --testPathPattern=tests/integration --runInBand",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.2",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "redis": "^5.8.2"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.1.7",
    "cross-env": "^7.0.3",
    "jest": "^30.2.0",
    "mongodb-memory-server": "^10.0.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.0.0"
  }
}
